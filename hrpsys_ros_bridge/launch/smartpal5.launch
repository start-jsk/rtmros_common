<launch>
  <arg name="KILL_SERVERS" default="false" />
  <include file="$(find hrpsys_ros_bridge)/launch/smartpal5_startup.launch" >
    <arg name="KILL_SERVERS" default="$(arg KILL_SERVERS)" />
  </include>
  <include file="$(find hrpsys_ros_bridge)/launch/smartpal5_ros_bridge.launch" />

  <sphinxdoc><![CDATA[


This launch file shows an example of ros bridge for yaskawa smartpal5 robot.

.. video:: build/images/SmartPal5_PickUp
  :width: 600

.. video:: build/images/smartpal5_rviz
  :width: 600


  ]]></sphinxdoc>
  <test type="test-grxui.py" pkg="openhrp3" time-limit="600"
   test-name="SmartPal5_PickUp"
   args="--max-time=10
	 --viewer-name='' #
	 --no-start-simulation
	 --capture-window='Irrlicht'
	 --target-directory=$(find hrpsys_ros_bridge)/build/images
	 --check-tf='/WAIST_LINK0 /VISION_SENSOR1'
	 "/> <!-- add dummy __log:= argument to invoke roseus with non-interactive mode -->
  <test type="ogv_encode.sh" pkg="jsk_tools" test-name="z_SmartPal5_PickUp" args="$(find hrpsys_ros_bridge)/build/images/SmartPal5_PickUp.ogv" time-limit="1000"/>
  <test type="glc_encode.sh" pkg="jsk_tools" test-name="z_smartpal5_rviz" args="$(find hrpsys_ros_bridge)/build/images/smartpal5_rviz.glc" time-limit="2000"/>
</launch>


