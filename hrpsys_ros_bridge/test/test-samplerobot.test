<launch>

  <node name="start_omninames" pkg="rtmbuild" type="start_omninames.sh" args="2809" />

  <include file="$(find hrpsys_ros_bridge)/launch/samplerobot.launch" >
    <arg name="corbaport" default="2809" />
    <arg name="GUI" default="false" />
    <arg name="RUN_RVIZ" default="false" />
  </include>

  <!-- check if tf is published -->       
  <env name="RTC_CONNECTION_CHECK_ONCE" value="true" />

  <param name="hztest_tf/topic" value="/joint_states" />
  <param name="hztest_tf/wait_time" value="240" />
  <param name="hztest_tf/hz" value="500.0" />
  <param name="hztest_tf/hzerror" value="50.0" />
  <param name="hztest_tf/test_duration" value="120.0" />
  <test test-name="hztest_tf" pkg="rostest" type="hztest" name="hztest_tf" retry="0" time-limit="600" />
</launch>
