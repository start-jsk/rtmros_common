<launch>
  <arg name="CORBAPORT" default="2809" />
  <arg name="GUI" default="false" />
  <arg name="HZ_WRENCHSTAMPED_TOPIC" default="10" />

  <include file="$(find hrpsys_ros_bridge)/launch/hrpsys_ros_bridge.launch">
    <arg name="corbaport" value="$(arg CORBAPORT)" />
    <arg name="GUI" value="$(arg GUI)" />
    <arg name="HZ_WRENCHSTAMPED_TOPIC" value="$(arg HZ_WRENCHSTAMPED_TOPIC)" />
  </include>

  <test pkg="hrpsys_ros_bridge" type="TestHrpsysSeqstateROSBridge" test-name="testname_hrpsysSeqstateROSBridge" name="hrpsysSeqstateROSBridge_node" time-limit="120"/>

</launch>
