<package>
  <name>rtmbuild</name>
  <version>1.0.11</version>
  <description>Build scripts for OpenRTM and OpenHRP</description>
  <license>BSD</license>
  <maintainer email="k-okada@jsk.t.u-tokyo.ac.jp">Kei Okada</maintainer>
